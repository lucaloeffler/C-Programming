"""

"""

#include <stdio.h>
#include <tgmath.h>

int main(void) {
  double celsius = 1;
  double fahrenheit = 1;
  double kelvin = 1;
  
  printf("Temperatur in Grad Celsius?:\n"); //Statement frage
  scanf("%lf", &celsius);
  fahrenheit = ( ( celsius * 9 ) / 5 ) + 32;//Formel zu Berechnung von Fahrenheit zu Kelvin 
  kelvin = (celsius + 273,15);
  printf("%.2lf Fahrenheit\n", fahrenheit); //print statement 
  printf("%.2lf Kelvin", kelvin);
  return 0;
}
